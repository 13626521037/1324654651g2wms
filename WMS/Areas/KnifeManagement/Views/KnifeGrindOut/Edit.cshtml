
@model WMS.ViewModel.KnifeManagement.KnifeGrindOutVMs.KnifeGrindOutVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model" url="/KnifeManagement/KnifeGrindOut/Edit" >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.DocNo"/>
      <wt:combobox field="Entity.HandledById"  item-url="/KnifeManagement/KnifeGrindOut/GetFrameworkUsers" label-text="@Localizer["Page.经办人"].Value" empty-text="@Localizer["Sys.PleaseSelect"]" id="g4c06d633311d42b180665abaff0cca37" multi-select="false"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:combobox field="Entity.Status" empty-text="@Localizer["Sys.PleaseSelect"]" id="g848a1ffbfc384f7ea70413b7c7f30209" multi-select="false"/>
      <wt:datetime field="Entity.ApprovedTime"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:grid use-local-data="true" hidden-panel="true" hidden-checkbox="true" height="-400"  vm="KnifeGrindOutLineKnifeGrindOutList1" label-text="@Localizer["Page.刀具修磨出库单"].Value" hide-label="true"/>
      <wt:submitbutton  text="@Localizer["Sys.Submit"].Value"/>
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

    <wt:hidden field="Entity.ID" />
  </wt:form>

</wt:container>


