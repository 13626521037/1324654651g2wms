
@model WMS.ViewModel.PurchaseManagement.PurchaseOutsourcingReturnVMs.PurchaseOutsourcingReturnVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model" url="/PurchaseManagement/PurchaseOutsourcingReturn/Create" >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.CreatePerson"/>
      <wt:combobox field="Entity.OrganizationId"  item-url="/PurchaseManagement/PurchaseOutsourcingReturn/GetBaseOrganizations" label-text="@Localizer["Page.组织"].Value" empty-text="@Localizer["Sys.PleaseSelect"]" id="gc84108909533400a843b1f788e385c3f" multi-select="false"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:datetime field="Entity.BusinessDate"/>
      <wt:datetime field="Entity.SubmitDate"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.DocNo"/>
      <wt:textbox field="Entity.DocType"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:combobox field="Entity.SupplierId"  item-url="/PurchaseManagement/PurchaseOutsourcingReturn/GetBaseSuppliers" label-text="@Localizer["Page.供应商"].Value" empty-text="@Localizer["Sys.PleaseSelect"]" id="gf533a94d75b346aeb838a1bbb844f09c" multi-select="false"/>
      <wt:combobox field="Entity.Status" empty-text="@Localizer["Sys.PleaseSelect"]" id="g87e27f9b4292459d8dcda806dd13d497" multi-select="false"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.SourceSystemId"/>
      <wt:datetime field="Entity.LastUpdateTime"/>
    </wt:row>

    <wt:row  items-per-row="ItemsPerRowEnum.One">
      <wt:textarea field="Entity.Memo"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:submitbutton  text="@Localizer["Sys.Submit"].Value"/>
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


