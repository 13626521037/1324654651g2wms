
@model WMS.ViewModel.BaseData.BaseOrganizationVMs.BaseOrganizationVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model"  >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Code" disabled="true"/>
      <wt:display field="Entity.Name" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.IsProduction" disabled="true"/>
      <wt:display field="Entity.IsSale" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.IsEffective" disabled="true"/>
      <wt:display field="Entity.Memo" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.SourceSystemId" disabled="true"/>
      <wt:display field="Entity.LastUpdateTime" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.CreateTime" disabled="true"/>
      <wt:display field="Entity.UpdateTime" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.CreateBy" disabled="true"/>
      <wt:display field="Entity.UpdateBy" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.IsValid" disabled="true"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


