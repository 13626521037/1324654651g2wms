
@model WMS.ViewModel.PrintManagement.PrintDocumentVMs.PrintDocumentVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model"  >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.DocNo" disabled="true"/>
      <wt:display field="Entity.DocLineNo" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.ItemID" disabled="true"/>
      <wt:display field="Entity.ItemCode" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.ItemName" disabled="true"/>
      <wt:display field="Entity.SPECS" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Description" disabled="true"/>
      <wt:display field="Entity.UnitName" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Qty" disabled="true"/>
      <wt:display field="Entity.ReceivedQty" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.ValidQty" disabled="true"/>
      <wt:display field="Entity.Seiban" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.SeibanRandom" disabled="true"/>
      <wt:display field="Entity.CreateTime" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.UpdateTime" disabled="true"/>
      <wt:display field="Entity.CreateBy" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.UpdateBy" disabled="true"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


