
@model WMS.ViewModel.BaseData.BaseItemMasterVMs.BaseItemMasterVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model"  >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Organization.SourceSystemId" disabled="true" label-text="@Localizer["Page.组织"].Value"/>
      <wt:display field="Entity.ItemCategory.SourceSystemId" disabled="true" label-text="@Localizer["Page.分类"].Value"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Code" disabled="true"/>
      <wt:display field="Entity.Name" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.SPECS" disabled="true"/>
      <wt:display field="Entity.MateriaModel" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Description" disabled="true"/>
      <wt:display field="Entity.StockUnit.SourceSystemId" disabled="true" label-text="@Localizer["Page.库存单位"].Value"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.ProductionOrg.SourceSystemId" disabled="true" label-text="@Localizer["Page.默认生产组织"].Value"/>
      <wt:display field="Entity.ProductionDept.SourceSystemId" disabled="true" label-text="@Localizer["Page.默认生产部门"].Value"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Wh.SourceSystemId" disabled="true" label-text="@Localizer["Page.默认存储地点"].Value"/>
      <wt:display field="Entity.FormAttribute" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.MateriaAttribute" disabled="true"/>
      <wt:display field="Entity.GearRatio" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Power" disabled="true"/>
      <wt:display field="Entity.SafetyStockQty" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.FixedLT" disabled="true"/>
      <wt:display field="Entity.BuildBatch" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.NotAnalysisQty" disabled="true"/>
      <wt:display field="Entity.AnalysisType.SourceSystemId" disabled="true" label-text="@Localizer["Page.分析类别"].Value"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.IsEffective" disabled="true"/>
      <wt:display field="Entity.SourceSystemId" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.LastUpdateTime" disabled="true"/>
      <wt:display field="Entity.CreateTime" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.UpdateTime" disabled="true"/>
      <wt:display field="Entity.CreateBy" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.UpdateBy" disabled="true"/>
      <wt:display field="Entity.IsValid" disabled="true"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


