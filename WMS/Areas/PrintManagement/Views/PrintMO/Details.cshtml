
@model WMS.ViewModel.PrintManagement.PrintMOVMs.PrintMOVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model"  >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.CustomerCode" disabled="true"/>
      <wt:display field="Entity.OrderWhName" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.CustomerSPECS" disabled="true"/>
      <wt:display field="Entity.Item.SourceSystemId" disabled="true" label-text="@Localizer["Page.料品"].Value"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.Seiban" disabled="true"/>
      <wt:display field="Entity.SeibanRandom" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.BatchNumber" disabled="true"/>
      <wt:display field="Entity.Qty" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.DocNo" disabled="true"/>
      <wt:display field="Entity.DocNoChange" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.LocationName" disabled="true"/>
      <wt:display field="Entity.Org.SourceSystemId" disabled="true" label-text="@Localizer["Page.组织"].Value"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.SyncItem" disabled="true"/>
      <wt:display field="Entity.SyncOrg" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.CreateTime" disabled="true"/>
      <wt:display field="Entity.UpdateTime" disabled="true"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:display field="Entity.CreateBy" disabled="true"/>
      <wt:display field="Entity.UpdateBy" disabled="true"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


