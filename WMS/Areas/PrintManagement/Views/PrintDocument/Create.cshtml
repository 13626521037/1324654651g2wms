
@model WMS.ViewModel.PrintManagement.PrintDocumentVMs.PrintDocumentVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model" url="/PrintManagement/PrintDocument/Create" >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.DocNo"/>
      <wt:textbox field="Entity.DocLineNo"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.ItemID"/>
      <wt:textbox field="Entity.ItemCode"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.UnitName"/>
      <wt:textbox field="Entity.Qty"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.ReceivedQty"/>
      <wt:textbox field="Entity.ValidQty"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.Seiban"/>
      <wt:textbox field="Entity.SeibanRandom"/>
    </wt:row>

    <wt:row  items-per-row="ItemsPerRowEnum.One">
      <wt:textarea field="Entity.ItemName"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.One">
      <wt:textarea field="Entity.SPECS"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.One">
      <wt:textarea field="Entity.Description"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:submitbutton  text="@Localizer["Sys.Submit"].Value"/>
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


