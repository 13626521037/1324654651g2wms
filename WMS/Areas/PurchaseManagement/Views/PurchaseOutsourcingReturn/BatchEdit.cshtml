
@model WMS.ViewModel.PurchaseManagement.PurchaseOutsourcingReturnVMs.PurchaseOutsourcingReturnBatchVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model" url="/PurchaseManagement/PurchaseOutsourcingReturn/DoBatchEdit?1=1">
  <div style="margin-bottom:10px;"> "@Localizer["Sys.BatchEditConfirm"].Value"</div>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:textbox field="LinkedVM.CreatePerson"/>
    <wt:combobox field="LinkedVM.OrganizationId"  item-url="/PurchaseManagement/PurchaseOutsourcingReturn/GetBaseOrganizations" label-text="@Localizer["Page.组织"].Value" id="ga215f5eef7504b2ba2c10bbe614b454f"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:datetime field="LinkedVM.BusinessDate"/>
    <wt:datetime field="LinkedVM.SubmitDate"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:textbox field="LinkedVM.DocNo"/>
    <wt:textbox field="LinkedVM.DocType"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:combobox field="LinkedVM.SupplierId"  item-url="/PurchaseManagement/PurchaseOutsourcingReturn/GetBaseSuppliers" label-text="@Localizer["Page.供应商"].Value" id="gaaca0886f41c483dac40931c8a77f5a4"/>
    <wt:combobox field="LinkedVM.Status" id="g3820fae289d84ccdab1c37d4f6d6ba2e"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:textarea field="LinkedVM.Memo"/>
  </wt:row>

  <wt:container align="AlignEnum.Right">
    <wt:submitbutton  text="@Localizer["Sys.Submit"].Value"/>
    <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
  </wt:container>

  <wt:hidden field="Ids" />
  <wt:grid vm="ListVM" use-local-data="true" height="300" hidden-checkbox="true" hidden-panel="true" />
</wt:form>


