@model WMS.ViewModel.BaseData.BaseSysParaVMs.BaseSysParaVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:textbox field="Entity.Code" disabled="true" />
        <wt:textbox field="Entity.Name" disabled="true" />
        <wt:textbox field="Entity.Value" />
        <wt:textbox field="Entity.Memo" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
