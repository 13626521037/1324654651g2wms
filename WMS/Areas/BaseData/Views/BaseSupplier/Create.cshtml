
@model WMS.ViewModel.BaseData.BaseSupplierVMs.BaseSupplierVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:container>
  <wt:form vm="@Model" url="/BaseData/BaseSupplier/Create" >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.Code"/>
      <wt:textbox field="Entity.Name"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:textbox field="Entity.ShortName"/>
      <wt:combobox field="Entity.OrganizationId"  item-url="/BaseData/BaseSupplier/GetBaseOrganizations" label-text="@Localizer["Page.组织"].Value" empty-text="@Localizer["Sys.PleaseSelect"]" id="g077056cb02aa45b8aea8f5df3c33ba9f" multi-select="false"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:combobox field="Entity.IsEffective" empty-text="@Localizer["Sys.PleaseSelect"]" id="gb993a613315c4f2c8469df465681fc06" multi-select="false"/>
      <wt:textbox field="Entity.SourceSystemId"/>
    </wt:row>
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:datetime field="Entity.LastUpdateTime"/>
    </wt:row>

    <wt:row  items-per-row="ItemsPerRowEnum.One">
      <wt:textarea field="Entity.Memo"/>
    </wt:row>

    <wt:container align="AlignEnum.Right">
      <wt:submitbutton  text="@Localizer["Sys.Submit"].Value"/>
      <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
    </wt:container>

  </wt:form>

</wt:container>


