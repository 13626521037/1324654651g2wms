
@model WMS.ViewModel.PurchaseManagement.PurchaseReturnVMs.PurchaseReturnBatchVM
@using WalkingTec.Mvvm.Core.Extensions;
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model" url="/PurchaseManagement/PurchaseReturn/DoBatchEdit?1=1">
  <div style="margin-bottom:10px;"> "@Localizer["Sys.BatchEditConfirm"].Value"</div>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:textbox field="LinkedVM.LastUpdatePerson"/>
    <wt:combobox field="LinkedVM.OrganizationId"  item-url="/PurchaseManagement/PurchaseReturn/GetBaseOrganizations" label-text="@Localizer["Page.组织"].Value" id="g9586e35655ca4671bc8a49afd8da7154"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:datetime field="LinkedVM.BusinessDate"/>
    <wt:datetime field="LinkedVM.CreateDate"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:textbox field="LinkedVM.DocType"/>
    <wt:combobox field="LinkedVM.SupplierId"  item-url="/PurchaseManagement/PurchaseReturn/GetBaseSuppliers" label-text="@Localizer["Page.供应商"].Value" id="gb70d2503a1b44144b944220516f2c09f"/>
  </wt:row>
  <wt:row  items-per-row="ItemsPerRowEnum.Two">
    <wt:combobox field="LinkedVM.Status" id="ge6c7fae03a8e4ad3b14dfba3bf6267ae"/>
    <wt:textarea field="LinkedVM.Memo"/>
  </wt:row>

  <wt:container align="AlignEnum.Right">
    <wt:submitbutton  text="@Localizer["Sys.Submit"].Value"/>
    <wt:closebutton  text="@Localizer["Sys.Close"].Value"/>
  </wt:container>

  <wt:hidden field="Ids" />
  <wt:grid vm="ListVM" use-local-data="true" height="300" hidden-checkbox="true" hidden-panel="true" />
</wt:form>


